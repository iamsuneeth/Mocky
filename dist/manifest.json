{
    "manifest_version": 2,
    "name": "My Extension",
    "version": "1.0",
    "description": "A plain text description",
    "author": "suneeth",
    "background": { "scripts": ["background.js"] },
    "permissions": [
        "webRequest",
        "webRequestBlocking",
        "storage",
        "*://*/*"
      ],
      "devtools_page": "devtools.html",
      "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
  }